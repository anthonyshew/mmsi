{
    "name": "mmsi",
    "version": "0.1.1",
    "description": "Mission Mechanical Services Inc. Website",
    "engines": {
        "node": "13.x",
        "npm": "6.x"
    },
    "scripts": {
        "start": "node server",
        "dev": "concurrently --kill-others-on-fail \" cd server && nodemon index.js \" \" cd client && npm run start \"",
        "devbuild": "cd client && npm run build && cd .. && cd server && nodemon index.js",
        "build": "cd client && npm install && npm run build"
    },
    "cacheDirectories": [
        "node_modules",
        "client/node_modules"
    ],
    "dependencies": {
        "@sendgrid/mail": "^6.5.2",
        "aws-sdk": "^2.619.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "chalk": "^2.4.2",
        "express": "^4.16.4",
        "gatsby-plugin-express": "^1.1.6",
        "jsonwebtoken": "^8.5.1",
        "mailchimp-api-v3": "^1.13.1",
        "mongoose": "^5.9.2",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "pg": "^7.12.1",
        "stripe": "^7.9.1",
        "validator": "^11.1.0"
    },
    "devDependencies": {
        "concurrently": "^4.1.2",
        "dotenv": "^8.1.0",
        "nodemon": "^1.19.3"
    }
}